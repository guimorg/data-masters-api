{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Output Schema for Model",
  "required": [
    "score",
    "label"
  ],
  "properties": {
    "score": {
      "$id": "#/properties/score",
      "type": "number",
      "title": "Score prediction",
      "examples": [
        98.7
      ]
    },
    "label": {
      "$id": "#/properties/label",
      "type": "string",
      "title": "Label given score",
      "default": "",
      "examples": [
        "POSITIVE"
      ],
      "pattern": "^(.*)$"
    }
  }
}
